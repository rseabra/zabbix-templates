                <item>
                    <uuid>adf61be122494c93879f16156e1dee0b</uuid>
                    <name>Cron lock</name>
                    <key>vfs.file.time[{$LOCKPATH},change]</key>
                    <delay>60s</delay>
                    <units>s</units>
                    <tags>
                        <tag>
                            <tag>Application</tag>
                            <value>Nextcloud</value>
                        </tag>
                    </tags>
                    <triggers>
                        <trigger>
                            <uuid>1ae2fbfc97d64bd8bf7215e0db91a8c6</uuid>
                            <expression>(max(/Template App Nextcloud/vfs.file.time[{$LOCKPATH},change],{$LOCKTIMECRITICAL})-min(/Template App Nextcloud/vfs.file.time[{$LOCKPATH},change],{$LOCKTIMECRITICAL}))=0</expression>
                            <name>NextCloud cron not running!</name>
                            <priority>HIGH</priority>
                            <description>NextCloud cron not running for over {$LOCKTIMECRITICAL} seconds.</description>
                        </trigger>
                        <trigger>
                            <uuid>7d160c56e0fd49669c790720ad284e80</uuid>
                            <expression>(max(/Template App Nextcloud/vfs.file.time[{$LOCKPATH},change],{$LOCKTIMEWARN})-min(/Template App Nextcloud/vfs.file.time[{$LOCKPATH},change],{$LOCKTIMEWARN}))=0</expression>
                            <name>NextCloud cron not running!</name>
                            <priority>WARNING</priority>
                            <description>NextCloud cron not running for over {$LOCKTIMEWARN} seconds.</description>
                        </trigger>
                    </triggers>
                </item>
